{
  "name": "rpc-apitest",
  "version": "0.3.28",
  "description": "RPC Apitest - an Isomorphic way to test APIs, from Web, Browser Console or Nodejs Debugger.",
  "main": "rpc-apitest.js",
  "bin": {
    "rpc-apitest": "bin/rpc-run.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --max-http-header-size=81000 --inspect ./rpc-apitest.js",
    "client": "rollup --config ./client/rollup.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitmplay/rpc-apitest.git"
  },
  "keywords": [
    "Remote-Procedure-Call Api-Test Swagger OpenAPI"
  ],
  "author": "Widi Harsojo",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "git+https://github.com/mitmplay/rpc-apitest.git"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "ansi-colors": "^4.1.3",
    "chance": "^1.1.11",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "highlight.js": "^11.7.0",
    "https": "^1.0.0",
    "json-schema-faker": "^0.5.0-rcv.46",
    "json-schema-ref-parser": "^9.0.9",
    "jsonrpc-lite": "^2.2.0",
    "knex": "^2.4.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^3.0.0",
    "mysql2": "^3.2.0",
    "open": "^9.0.0",
    "request": "^2.88.2",
    "rollup": "^3.19.1",
    "sqlite3": "5.0.0",
    "tabulator-tables": "^5.4.4",
    "ws": "^8.12.1",
    "yaml": "^2.2.1",
    "yargs-parser": "^21.1.1"
  },
  "engines": {
    "node": ">=18.11.0"
  }
