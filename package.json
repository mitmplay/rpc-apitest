    "knex": "^3.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "markdown-it-container": "^4.0.0",
    "mysql2": "^3.11.0",
    "open": "^10.1.0",
    "request": "^2.88.2",
    "rollup": "^3.19.1",
    "sqlite3": "^5.1.7",
    "tabulator-tables": "^6.2.5",
    "ws": "^8.18.0",
    "yaml": "^2.5.0",
    "yargs-parser": "^21.1.1"
  },
  "engines": {
    "node": ">=18.11.0"
  }
