{
  "name": "rpc-apitest",
  "version": "0.4.33",
  "description": "RPC Apitest - an Isomorphic way to test APIs, from Web, Browser Console or Nodejs Debugger.",
  "main": "rpc-apitest.js",
  "bin": {
    "rpc-apitest": "bin/rpc-run.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --max-http-header-size=81000 --inspect ./rpc-apitest.js",
    "client": "rollup --config ./client/rollup.config.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitmplay/rpc-apitest.git"
  },
  "keywords": [
    "Remote-Procedure-Call Api-Test Swagger OpenAPI"
  ],
  "author": "Widi Harsojo",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "git+https://github.com/mitmplay/rpc-apitest.git"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.7.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "ansi-colors": "^4.1.3",
    "chance": "^1.1.12",
    "chokidar": "^3.6.0",
    "date-fns": "3.6.0",
    "express": "^4.19.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "highlight.js": "^11.10.0",
    "html-prettify": "1.0.7",
    "https": "^1.0.0",
    "json-schema-faker": "^0.5.6",
